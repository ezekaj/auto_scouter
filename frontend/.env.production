# Production Environment Configuration for Auto Scouter Frontend

# API Configuration - Render Production URL
VITE_API_BASE_URL=https://auto-scouter-backend.onrender.com/api/v1
VITE_API_URL=https://auto-scouter-backend.onrender.com/api/v1
VITE_WS_BASE_URL=wss://auto-scouter-backend.onrender.com/ws
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Auto Scouter
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security Configuration
VITE_ENABLE_HTTPS_ONLY=true
VITE_ENABLE_STRICT_CSP=true

# External Services (Replace with actual values)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Cache Configuration
VITE_CACHE_VERSION=1.0.0

# Build Configuration
VITE_BUILD_TIMESTAMP=${BUILD_TIMESTAMP}
VITE_GIT_COMMIT=${GIT_COMMIT}

# Firebase Configuration for Push Notifications
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=vehicle-scout-notifications.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=vehicle-scout-notifications
VITE_FIREBASE_STORAGE_BUCKET=vehicle-scout-notifications.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_VAPID_KEY=your-vapid-key

# Cloud Features
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_CLOUD_SYNC=true
VITE_ENABLE_OFFLINE_MODE=true
